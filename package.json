  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.0.1",
    "@types/node": "22.10.1",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "@vercel/postgres": "^0.10.0",
    "bcrypt": "^5.1.1",
    "eslint": "9.16.0",
    "eslint-config-next": "15.0.4",
    "next": "15.0.4",
    "next-auth": "^4.24.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "typescript": "5.7.2",
    "vercel": "^39.1.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prisma": "^6.0.1",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2"
  }
